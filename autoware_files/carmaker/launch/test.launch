<launch>
  <!-- op_behavior_selector -->
  <include file="$(find op_local_planner)/launch/op_behavior_selector.launch">
    <arg name="evidence_tust_number"            default="25"/>
    <arg name="distanceToPedestrianThreshold"   default="20.0" />
    <arg name="sprintSpeed"                    default="10.0" />
    <arg name="obstacleWaitingTimeinIntersection"  default="2.0" />
    <arg name="turnThreshold"                   default="30.0" />
    <arg name="trafficLightFile"                default="sangam_scene2/traffic_light.yaml" />
    <arg name="stopLineFile"                    default="sangam_scene2/stop_line.yaml" />
  </include>
</launch>